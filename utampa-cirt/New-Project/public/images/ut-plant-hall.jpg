        <script>
    $(document).ready(function() {
        var alerts = JSON.parse('["https://www.getrave.com/rss/ut/channel1","https://www.getrave.com/rss/ut/channel2","https://www.getrave.com/rss/ut/channel3","https://www.getrave.com/rss/ut/channel4"]');

        for (var i = 0; i < alerts.length; i++) {
            try {
                parseRSSFeed(alerts[i], i);
            }
            catch (error) {
                console.error(error);
            }
        }

        function parseRSSFeed(RssURL, index) {
            var feed = RssURL;
            var descColor = "";
            var description = ""
            var link = "";

            $.ajax(feed, {
                accepts: {
                    xml: "application/rss+xml"
                },
                dataType: "xml",
                success: function (data) {

                    $(data).find("channel").each(function () { // or "item" or whatever suits your feed
                        var el = $(this);
                        descColor = el.find("description").first().text();
                        link = el.find("link").last().text();
                        description = el.find("description").last().text();
                        if (description.toLowerCase() !== "all clear") {
                            var alertClass = "";
                            var linkString = "";
                            var hColor = "";
                            var aColor = "";
                            var buttonColor = "";
                            description = description.replace(/(<([^>]+)>)/gi, "");
                            var alertindex = "alert" + index;
                            var switchOne = descColor.substring(descColor.lastIndexOf('-') + 1, descColor.length).trim();
                            switch (switchOne) {
                                case "Red":
                                    alertClass = "primary";
                                    hColor = "white-text";
                                    aColor = "white-text";
                                    buttonColor = "red-button-text";
                                    break;
                                case "Yellow":
                                    alertClass = "secondary";
                                    hColor = "black-text";
                                    aColor = "black-text"
                                    buttonColor = "white-button-text";
                                    break;
                                case "Green":
                                    alertClass = "accent-1";
                                    hColor = "";
                                    aColor = "black-text"
                                    buttonColor = "white-button-text";
                                    break;
                                default:
                                    alertClass = "accent-1";
                                    hColor = "black-text";
                                    aColor = "black-text";
                                    buttonColor = "black-button-text";
                                    break;
                            }
                            if (link != null && link !== "") {
                                linkString = "<div class=\"button-rave\"><a class=\"" + buttonColor + "\" href=\"" + link + "\">More</a></div>";
                            }
                            description = "<div class=" + hColor + ">" + description +"</div>";
                            document.querySelector("#rave").insertAdjacentHTML(
                                'afterbegin',
                                "<section id='" + alertindex + "' class='tray tray-alert " + alertClass + "'>" +
                                "<div class='container'>" +
                                "<div class='content'><div class=\"rave-alert-left\"><i class=\"fa fa-err-outline\"></i></div>" +
                                "<div class='rave-alert-center'>" +
                                description +
                                linkString +
                                "</div>" +
                                "<div class=\"rave-alert-right\"><a class=\"close-alert " + aColor + "\" title=\"Close alert\" href=\"#\" style=\"cursor:pointer;\" onclick=\"closeAlert('" + alertindex + "');return false;\"><i class=\"fas fa-times\"></i></a></div>" +
                                "</div>" +
                                "</div>" +
                                "</div>"
                            );
                            $(".header").addClass("rave-alert");
                            $(".main.container").addClass("rave-alert");
                        }
                    });
                }
            });
        }
    });
    function closeAlert(alertID) {
        document.getElementById(alertID).remove();
        $(".header").removeClass("rave-alert");
        $(".main.container").removeClass("rave-alert");
    }
</script>
